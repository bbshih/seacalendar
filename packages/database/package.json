{
  "name": "@seacalendar/database",
  "version": "2.0.0",
  "description": "Database schema and Prisma client for SeaCalendar",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "generate": "dotenv -e ../../.env.development -e ../../.env.development.local -- prisma generate",
    "migrate:dev": "dotenv -e ../../.env.development -e ../../.env.development.local -- prisma migrate dev",
    "migrate:deploy": "dotenv -e ../../.env.production -e ../../.env.production.local -- prisma migrate deploy",
    "migrate:reset": "dotenv -e ../../.env.development -e ../../.env.development.local -- prisma migrate reset --force",
    "studio": "dotenv -e ../../.env.development -e ../../.env.development.local -- prisma studio",
    "seed": "dotenv -e ../../.env.development -e ../../.env.development.local -- tsx prisma/seed.ts",
    "reset": "npm run migrate:reset && npm run seed",
    "build": "tsc"
  },
  "dependencies": {
    "@prisma/client": "^5.9.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "dotenv-cli": "^7.3.0",
    "prisma": "^5.9.1",
    "tsx": "^4.7.0",
    "typescript": "^5.9.3"
  }
}
