    "seed": "tsx prisma/seed.ts",
    "reset": "npm run migrate:reset && npm run seed",
    "build": "tsc"
  },
  "dependencies": {
    "@prisma/client": "^6.18.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "prisma": "^5.9.1",
    "tsx": "^4.7.0",
    "typescript": "^5.9.3"
  }
}
